{
  "name": "cteh-interview-repo",
  "description": "Rails/GraphQL/ReactJS/Relay Repository for Pair Programming Interview",
  "version": "1.0.0",
  "repository": "RailsGraphQL Lab",
  "license": "Owned by CTEH LLC",
  "scripts": {
    "start": "yarn run webpack & yarn run rails & yarn run watch",
    "webpack": "`yarn bin`/webpack -w -d --devtool source-map",
    "watch": "yarn run schema:dump_schema; yarn run schema:trigger_webpack; yarn run schema:watch",
    "schema:watch": "`yarn bin`/watch -p '**/*.rb' -c 'yarn run schema:dump_schema; yarn run schema:trigger_webpack'",
    "schema:dump_schema": "rake relay:dump_schema",
    "schema:trigger_webpack": "echo \"module.exports = { schemaUpdatedOn: '`Date`' }\" > app/client/schemaUpdatedOn.js",
    "rails": "rails s",
    "lint": "`yarn bin`/eslint 'app/**/*.@(js|jsx)'",
    "test": "rspec"
  },
